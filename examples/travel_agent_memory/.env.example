# Google Gemini API Key (required)
GOOGLE_API_KEY=your-google-api-key-here

# Tavily API Key (optional - for web search functionality)
TAVILY_API_KEY=your-tavily-api-key-here

# Agent Memory Server URL (optional - defaults to http://localhost:8088)
MEMORY_SERVER_URL=http://localhost:8088

# Redis URL (optional - defaults to redis://localhost:6379)
REDIS_URL=redis://localhost:6379

